<div class="container">
  <h1>Our Team</h1>
</div>
<div class="container">
  <div class="row">
    <div class="col-sm-8 col-md-9">
      <section class="photo-grid">
        <ul class="team-photos">
        {{# each data.members }}
          <li id="{{key}}" class="person">
            {{#if images.0.url}}
            <img src="{{images.0.url}}" class="person"/>
            {{ else }}
            <img src="http://www.clipartbest.com/cliparts/RiG/dxk/RiGdxkAiL.jpeg" class="person"/>
            {{/if}}
            <div class="">
            </div>
          </li>
        {{/each}}
        </ul>
      </section>
    </div>
  </div>
</div>
