<section id="team" class="photo-grid">
  <div class="container-fluid">
    <div class="row brief">
      <div class="col-xs-12 text-center"><h3>Say hello to the future of digital commerce.</h3></div>
    </div>
    <div class="row">
      <ul class="team-photos">
      {{# each data.members }}
        <li id="{{key}}" class="person">
          {{#if images.0.url}}
          <div class="person" style="background-image: url('{{images.0.url}}')" />
          {{ else }}
          <img src="http://kloofvillagedental.co.za/wp-content/uploads/2011/11/blank-profile-hi-300x300.png" class="person"/>
          {{/if}}
        </li>
      {{/each}}
      </ul>
    </div>
  </div>
</section>
